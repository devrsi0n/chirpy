{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.fontSize": 16,
  "cSpell.words": [
    "blacka",
    "centralus",
    "grayd",
    "grayl",
    "jsonb",
    "maizzle",
    "pnpm",
    "signin",
    "tsconfigs",
    "visx",
    "whitea",
    "wonka"
  ],
  "tailwindcss.preferVariantWithParentheses": true,
  "jestrunner.configPath": "jest.config.js",
  "jest.jestCommandLine": "npm run test --",
  "svg.preview.background": "white",
  "tailwindCSS.experimental.configFile": "apps/main/tailwind.config.js"
}
