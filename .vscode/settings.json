{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.fontSize": 16,
  "cSpell.words": ["blacka", "centralus", "grayd", "grayl", "jsonb", "visx", "whitea", "wonka"],
  "tailwindcss.preferVariantWithParentheses": true,
  "jestrunner.configPath": "jest.config.js",
  "jest.jestCommandLine": "npm run ut --",
  "eslint.workingDirectories": ["./src"],
  "eslint.alwaysShowStatus": true,
  "svg.preview.background": "white"
}
