{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.fontSize": 16,
  "cSpell.words": [
    "blacka",
    "centralus",
    "jsonb",
    "visx",
    "whitea"
  ],
  "tailwindcss.preferVariantWithParentheses": true,
  "jestrunner.configPath": "jest.config.js",
  "jest.jestCommandLine": "npm run ut --"
}
