    "cookie": "^0.4.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jose": "^1.28.0",
    "next": "^9.5.3",
    "next-connect": "^0.9.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0"
