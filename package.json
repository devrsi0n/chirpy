{
  "name": "totalk",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:both": "concurrently 'next dev' 'npm run dev:external'",
    "dev:external": "vite --config scripts/vite.config.js",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently 'next build' 'npm run build:external'",
    "build:external": "vite build --config scripts/vite.build.config.js",
    "build:external:dev": "vite build --config scripts/vite.dev.config.js",
    "build:local": "dotenv -e .env.development -- npm run build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:local": "dotenv -e .env.development -- next start",
    "g:gql": "graphql-codegen --config ./docs/server-codegen.yml && graphql-codegen --config ./docs/client-codegen.yml",
    "g:gql:watch": "concurrently 'graphql-codegen --watch --config ./docs/server-codegen.yml' 'graphql-codegen --config ./docs/client-codegen.yml --watch'",
    "cypress:open": "CYPRESS_BASE_URL=http://localhost:3000 cypress open",
    "cypress:run": "CYPRESS_BASE_URL=http://localhost:3000 cypress run",
    "ut": "DEBUG_PRINT_LIMIT=10000 jest --silent=false",
    "ut:coverage": "DEBUG_PRINT_LIMIT=10000 jest --silent=false --coverage",
    "lint": "next lint"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{json,md,scss,css}": [
      "prettier --write",
      "npm run g:gql"
    ],
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
