{
  "private": true,
  "scripts": {
    "dev": "concurrently \"next\" \"parcel watch ./src/lib/comment.ts --out-dir public\"",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently \"next build\" \"parcel build ./src/lib/comment.ts --out-dir public --no-source-maps\"",
    "build:local": "dotenv -e .env.development -- concurrently \"next build\" \"parcel build ./src/lib/comment.ts --out-dir public --no-source-maps\"",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "migrate:save": "dotenv -e .env.development -- prisma migrate save --experimental",
    "migrate:up": "dotenv -e .env.development -- prisma migrate up --experimental --verbose",
    "migrate:rollback": "dotenv -e .env.development -- prisma migrate rollback --experimental",
    "studio": "dotenv -e .env.development -- prisma studio",
    "g:prisma": "prisma generate",
    "g:gql": "graphql-codegen --config graphql-codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "@geist-ui/react-icons": "^1.0.1",
    "@hapi/iron": "^6.0.0",
