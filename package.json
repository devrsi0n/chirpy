    "@prisma/cli": "2.9.0",
    "@types/cookie": "^0.4.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "^14.14.2",
    "@types/passport": "^1.0.4",
    "@types/passport-github": "^1.1.5",
    "@types/passport-github2": "^1.2.4",
