{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:parcel": "parcel watch ./src/external/comment.ts --out-dir public",
    "dev:both": "concurrently \"next\" \"parcel watch ./src/external/comment.ts --out-dir public\"",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "concurrently \"next build\" \"parcel build ./src/external/comment.ts --out-dir public --no-source-maps\"",
    "build:local": "dotenv -e .env.development -- concurrently \"next build\" \"parcel build ./src/external/comment.ts --out-dir public --no-source-maps\"",
    "analyze": "ANALYZE=true dotenv -e .env.development -- next build",
    "start": "next start",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev --preview-feature",
    "studio": "dotenv -e .env.development -- prisma studio",
    "g:prisma": "prisma generate",
    "g:gql": "graphql-codegen --config graphql-codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@geist-ui/react-icons": "^1.0.1",
    "@hapi/iron": "^6.0.0",
